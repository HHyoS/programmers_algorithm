#include <string>
#include <vector>
#include <algorithm>
#include <iostream>
using namespace std;

long long solution(int n, vector<int> times) {
    
    sort(times.begin(), times.end());
    long long left = 0, right = times[times.size()-1] * n;
    long long answer = right, mid = 0;
    long count = 0;
    while(left <= right){
        count = 0;
        mid = (left + right) / 2;
        for(int a : times)
            count = count + mid/a;
        if(count < n)
            left = mid+1;
        else{
            if(answer >= mid)
                answer = mid;
            right = mid-1;
        }
    }
            
    return answer;
}
