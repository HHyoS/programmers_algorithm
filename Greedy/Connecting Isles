#include <string>
#include <vector>
#include <algorithm>
using namespace std;

int solution(int n, vector<vector<int>> costs) {
    bool check[n];
    int answer = 0;
    vector<vector<int>> ar(n, vector<int>(n,0));
    vector<vector<int>> map(n, vector<int>(n,0));
    for(int a = 0; a < n; ++a){
        check[a] = false;
        for(int b = 0; b < n; ++b){
            if(a==b){
                ar[a][b] = 0;
                map[a][b] = a;
            }
            else
                ar[a][b] = 9999;
        }
    }
    for(auto c : costs){
        ar[c[0]][c[1]] = c[2];
    }
    for(int a = 0;  a < n; ++a){
        for(int b = 0; b < n; ++b){
                for(int c = a; c <= b; ++c){
                    if(ar[a][b] > ar[a][c]+ar[c][b]){
                        ar[a][b] = ar[a][c] + ar[c][b];
                        printf("%d->%d-%d = %d\n",a,c,b,ar[a][b]);
                        map[a][b] = c;
                    }
                    
            }
        }
    }
    
    
    return answer;
}
